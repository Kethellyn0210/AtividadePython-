programa {
  funcao inicio() {
    cadeia nome
    inteiro idade
    real peso
    real altura
    real imc

   escreva("Nome: ")
   leia(nome)

   escreva("Idade: ")
   leia(idade)

   escreva("Peso: ")
   leia(peso)

  escreva("Altura: ")
  leia(altura)

  imc = (altura + altura) / peso 

  escreva("IMC: ", imc)
  }
}